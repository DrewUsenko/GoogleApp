<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <subclass discriminator-value="Student" extends="slGal.LiveEdu.ORM.Person" name="slGal.LiveEdu.ORM.Student">
    <property name="faculty" column="faculty" type="string"/>
    <property name="facultyFull" column="facultyFull" type="string"/>
    <property name="course" column="course" type="int"/>
    <property name="spec" column="spec" type="string"/>
    <property name="numberSpec" column="numberSpec" type="string"/>
    <property name="card" column="studentCard" type="string"/>
    <property name="edbo" column="EDBO" type="string"/>
    <property name="groupa" column="groupa" type="string"/>
    <property name="msdnaa" column="msdnaa" type="boolean"/> 
    <property name="office365" column="office365" type="boolean"/>
    <property name="studies" column="studies" type="string"/>

  </subclass>
</hibernate-mapping>
